{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/interactions",
      "destination": "/api/interactions"
    },
    {
      "source": "/api/register-commands",
      "destination": "/api/register-commands"
    },
    {
      "source": "/api/health",
      "destination": "/api/health"
    },
    {
      "source": "/",
      "destination": "/api/health"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
